{
	"name": "pocket-stylist-ai",
	"version": "1.0.0",
	"description": "AI-powered fashion companion app built with React Native and Expo",
	"main": "App.js",
	"author": "YunosukeYoshino",
	"license": "MIT",
	"private": true,
	"packageManager": "bun@latest",
	"scripts": {
		"start": "expo start",
		"android": "expo run:android",
		"ios": "expo run:ios",
		"web": "expo start --web",
		"build": "eas build --platform all",
		"build:android": "eas build --platform android",
		"build:ios": "eas build --platform ios",
		"build:all": "eas build --platform all",
		"preview": "expo start --tunnel",
		"eject": "expo eject",
		"server": "bun run server/index.ts",
		"server:dev": "bun --watch run server/index.ts",
		"server:build": "tsc --project server/tsconfig.json",
		"type-check": "tsc --noEmit",
		"lint": "biome lint",
		"lint:fix": "biome lint --write",
		"format": "biome format --write",
		"check": "biome check",
		"check:fix": "biome check --write",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:server": "jest --selectProjects=\"Server API\"",
		"test:server:watch": "jest --selectProjects=\"Server API\" --watch",
		"test:client": "jest --selectProjects=\"React Native\"",
		"test:security": "jest --selectProjects=\"Server API\" --testPathPattern=security",
		"prepare": "husky install",
		"db:generate": "prisma generate && echo 'Generated Prisma client'",
		"db:migrate": "prisma migrate dev && echo 'Applied database migrations'",
		"db:migrate:deploy": "prisma migrate deploy && echo 'Deployed migrations to production'",
		"db:migrate:reset": "prisma migrate reset && echo 'Reset database schema'",
		"db:seed": "prisma db seed && echo 'Seeded database with sample data'",
		"db:studio": "prisma studio && echo 'Opening Prisma Studio'",
		"db:push": "prisma db push && echo 'Pushed schema changes to database'",
		"db:pull": "prisma db pull && echo 'Pulled schema from database'",
		"docker:up": "docker-compose up -d && echo 'Started Docker services'",
		"docker:down": "docker-compose down && echo 'Stopped Docker services'",
		"docker:logs": "docker-compose logs -f",
		"backend:install": "cd backend && npm install",
		"backend:dev": "cd backend && npm run dev",
		"backend:build": "cd backend && npm run build",
		"backend:start": "cd backend && npm start",
		"backend:test": "cd backend && npm test",
		"backend:lint": "cd backend && npm run lint",
		"backend:docker:up": "cd backend && docker-compose up -d",
		"backend:docker:down": "cd backend && docker-compose down",
		"dev:full": "concurrently \"npm run backend:dev\" \"npm run start\"",
		"setup": "npm install && npm run backend:install && npm run db:generate"
	},
	"dependencies": {
		"@prisma/client": "^6.12.0",
		"@tamagui/animations-react-native": "^1.132.11",
		"@tamagui/button": "^1.132.11",
		"@tamagui/config": "^1.132.11",
		"@tamagui/core": "^1.132.11",
		"@tamagui/font-inter": "^1.132.11",
		"@tamagui/input": "^1.132.11",
		"@tamagui/theme-base": "^1.132.11",
		"cors": "^2.8.5",
		"dotenv": "^17.2.0",
		"expo": "~53.0.20",
		"expo-auth-session": "~6.2.1",
		"expo-constants": "~17.1.7",
		"expo-crypto": "~14.1.5",
		"expo-font": "~13.3.2",
		"expo-linking": "~7.1.7",
		"expo-router": "~5.1.4",
		"expo-secure-store": "~14.2.3",
		"expo-splash-screen": "~0.30.10",
		"expo-status-bar": "~2.2.3",
		"expo-system-ui": "~5.0.10",
		"expo-web-browser": "~14.2.0",
		"express": "^5.1.0",
		"express-jwt": "^8.5.1",
		"express-rate-limit": "^8.0.1",
		"helmet": "^8.1.0",
		"jsonwebtoken": "^9.0.2",
		"jwks-rsa": "^3.2.0",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"react-native": "0.79.5",
		"react-native-auth0": "^4.6.0",
		"react-native-gesture-handler": "~2.24.0",
		"react-native-reanimated": "~3.17.4",
		"react-native-safe-area-context": "5.4.0",
		"react-native-screens": "~4.11.1",
		"react-native-svg": "15.11.2",
		"react-native-svg-transformer": "^1.5.1",
		"react-native-web": "~0.20.0",
		"zod": "^4.0.5",
		"zustand": "^5.0.6"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.2",
		"@expo/cli": "~0.24.20",
		"@expo/webpack-config": "~19.0.1",
		"@testing-library/react-native": "^13.2.0",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.3",
		"@types/glob": "^9.0.0",
		"@types/jest": "^30.0.0",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/minimatch": "^6.0.0",
		"@types/node": "^24.0.15",
		"@types/react": "~19.0.10",
		"@types/react-native": "~0.73.0",
		"@types/supertest": "^6.0.3",
		"concurrently": "^9.2.0",
		"expo-dev-client": "~5.2.4",
		"husky": "^9.1.7",
		"jest": "~29.7.0",
		"jest-expo": "~53.0.9",
		"lint-staged": "^16.1.2",
		"prisma": "^6.12.0",
		"supertest": "^7.1.3",
		"ts-jest": "^29.4.0",
		"typescript": "^5.8.3"
	},
	"lint-staged": {
		"!(backend|server)/**/*.{js,jsx,ts,tsx,json,css,md}": [
			"biome check --write"
		],
		"*.{js,jsx,ts,tsx,json,css,md}": [
			"biome check --write"
		]
	},
	"prisma": {
		"seed": "bun run prisma/seed.ts"
	}
}
