{
  "name": "pocket-stylist-ai",
  "version": "1.0.0",
  "description": "AI-powered fashion companion app built with React Native and Expo",
  "main": "expo/AppEntry.js",
  "author": "YunosukeYoshino",
  "license": "MIT",
  "private": true,
  "packageManager": "bun@latest",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "eas build --platform all",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "preview": "expo start --tunnel",
    "eject": "expo eject",
    "server": "bun run server/index.ts",
    "server:dev": "bun --watch run server/index.ts",
    "server:build": "tsc --project server/tsconfig.json",
    "type-check": "tsc --noEmit",
    "lint": "biome lint",
    "lint:fix": "biome lint --apply",
    "format": "biome format --write",
    "check": "biome check",
    "check:fix": "biome check --apply",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:server": "jest --selectProjects=\"Server API\"",
    "test:server:watch": "jest --selectProjects=\"Server API\" --watch",
    "test:client": "jest --selectProjects=\"React Native\"",
    "test:security": "jest --selectProjects=\"Server API\" --testPathPattern=security",
    "prepare": "husky install",
    "db:generate": "prisma generate && echo 'Generated Prisma client'",
    "db:migrate": "prisma migrate dev && echo 'Applied database migrations'",
    "db:migrate:deploy": "prisma migrate deploy && echo 'Deployed migrations to production'",
    "db:migrate:reset": "prisma migrate reset && echo 'Reset database schema'",
    "db:seed": "prisma db seed && echo 'Seeded database with sample data'",
    "db:studio": "prisma studio && echo 'Opening Prisma Studio'",
    "db:push": "prisma db push && echo 'Pushed schema changes to database'",
    "db:pull": "prisma db pull && echo 'Pulled schema from database'",
    "docker:up": "docker-compose up -d && echo 'Started Docker services'",
    "docker:down": "docker-compose down && echo 'Stopped Docker services'",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "@tamagui/animations-react-native": "^1.95.3",
    "@tamagui/button": "^1.131.3",
    "@tamagui/config": "^1.95.3",
    "@tamagui/core": "^1.95.3",
    "@tamagui/font-inter": "^1.95.3",
    "@tamagui/theme-base": "^1.95.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "express-rate-limit": "^7.1.5",
    "expo": "~50.0.0",
    "expo-constants": "~15.4.6",
    "expo-font": "~11.10.3",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.0",
    "expo-splash-screen": "~0.26.5",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.4",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-web": "~0.19.6",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.0",
    "@expo/cli": "~0.17.0",
    "@expo/webpack-config": "~19.0.0",
    "@testing-library/react-native": "^13.2.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/glob": "^9.0.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^20.10.6",
    "@types/react": "~18.2.45",
    "@types/react-native": "~0.73.0",
    "@types/supertest": "^6.0.2",
    "expo-dev-client": "~3.3.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-expo": "~50.0.0",
    "lint-staged": "^15.2.2",
    "prisma": "^6.11.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,md}": ["biome check --apply"]
  },
  "prisma": {
    "seed": "bun run prisma/seed.ts"
  }
}
