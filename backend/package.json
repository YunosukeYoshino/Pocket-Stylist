{
  "name": "pocket-stylist-backend",
  "version": "1.0.0",
  "description": "Backend API for Pocket Stylist AI with Claude integration",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --apply src/",
    "format": "biome format --write src/",
    "check": "biome check src/",
    "check:fix": "biome check --apply src/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.3",
    "@prisma/client": "^6.11.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "redis": "^4.6.13",
    "dotenv": "^16.4.5",
    "uuid": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.11.5",
    "@types/uuid": "^9.0.8",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/jest": "^29.5.12",
    "@types/node-cron": "^3.0.11",
    "@types/glob": "^9.0.0",
    "@types/minimatch": "^6.0.0",
    "@biomejs/biome": "^1.8.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "prisma": "^6.11.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "YunosukeYoshino",
  "license": "MIT"
}